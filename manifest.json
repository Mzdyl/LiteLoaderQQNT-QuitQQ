{
    "manifest_version": 4,
    "type": "extension",
    "name": "程序退出",
    "slug": "window_quit",
    "description": "改关闭按钮为退出",
    "version": "0.0.1",
    "authors": [
        {
            "name": "Mzdyl",
            "link": "https://github.com/mzdyl"
        }
    ],
    "repository": {
        "repo": "mzdyl/window-quit",
        "branch": "v4",
        "release": {
            "tag": "0.0.1"
        }
    },
    "platform": [
        "win32",
        "linux",
        "darwin"
    ],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    }
}